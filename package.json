{
  "name": "trustap-sdk",
  "version": "0.1.0",
  "description": "Trustap API SDK (OpenAPI)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./mod.ts": "./mod.ts"
  },
  "files": [
    "dist",
    "mod.ts",
    "index.ts",
    "client.ts",
    "webhook-schemas.ts",
    "state-machine.ts",
    "types.ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "generate": "openapi-typescript openapi.yaml -o types.ts",
    "check": "npm run generate && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "sideEffects": false,
  "dependencies": {
    "openapi-fetch": "^0.15.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3"
  }
}
