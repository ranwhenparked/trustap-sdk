{
  "name": "@ranwhenparked/trustap-sdk",
  "version": "0.2.0",
  "description": "Trustap API SDK (OpenAPI)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./webhooks": {
      "types": "./dist/webhooks/index.d.ts",
      "import": "./dist/webhooks/index.js"
    },
    "./types": {
      "types": "./dist/generated/types.d.ts",
      "import": "./dist/generated/types.js"
    },
    "./mod.ts": "./mod.ts"
  },
  "files": [
    "dist",
    "mod.ts"
  ],
  "scripts": {
    "generate": "openapi-typescript spec/openapi.yaml -o src/generated/types.ts",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run generate && npm run build",
    "fetch-spec": "curl -o spec/openapi.yaml https://docs.trustap.com/_bundle/apis/openapi.yaml",
    "sync": "npm run fetch-spec && npm run generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "openapi-fetch": "^0.15.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3"
  }
}